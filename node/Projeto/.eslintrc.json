
{
    "env": {
        "commonjs": true,
        "es2021": true,
        "node": true
    },
    "settings": {
        "import/resolver": {
          "node": {
            "extensions": [".jsx", ".ts", ".tsx"]
          }
        }
      },
    "extends": [
        "airbnb-base"
    ],
    "parserOptions": {
        "ecmaVersion": 12
    },
    "rules": {
      "no-param-reassign": [2, { "props": false }],
      "linebreak-style": 0,

      "import/no-unresolved": "off",
      "camelcase": "off"
    }
}

